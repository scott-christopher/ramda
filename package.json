  "name": "ramda",
  "description": "A practical functional library for JavaScript programmers.",
  "version": "0.19.1",
  "homepage": "http://ramdajs.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ramda/ramda.git"
  },
  "main": "src/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "bench": "scripts/benchRunner",
    "bookmarklet": "scripts/bookmarklet",
    "build": "npm run rollup && npm run minify",
    "build:cjs": "scripts/build-commonjs",
    "clean": "rimraf dist/* src",
    "lint": "eslint scripts/bookmarklet scripts/build es/*.js es/internal/*.js test/*.js test/**/*.js lib/sauce/*.js lib/bench/*.js",
    "pretest": "npm install && npm run lint && npm run clean && npm run build && npm run build:cjs",
    "test": "mocha --reporter spec",
    "posttest": "git checkout -- dist",
    "prebrowser_test": "npm run pretest",
    "browser_test": "testem ci",
    "postbrowser_test": "npm run posttest",
    "minify": "uglifyjs -o dist/ramda.min.js --compress --mangle -- dist/ramda.js",
    "rollup": "rollup -f umd -n R -i es/index.js -o dist/ramda.js",
    "release-major": "xyz --repo git@github.com:ramda/ramda.git --script scripts/prepublish --increment major",
    "release-minor": "xyz --repo git@github.com:ramda/ramda.git --script scripts/prepublish --increment minor",
    "release-patch": "xyz --repo git@github.com:ramda/ramda.git --script scripts/prepublish --increment patch"
  },
  "dependencies": {},
  "devDependencies": {
    "benchmark": "1.0.x",
    "browserify": "10.x.x",
    "cli-table": "0.3.x",
    "envvar": "1.x.x",
    "eslint": "1.10.x",
    "mocha": "2.x.x",
    "q": "1.1.x",
    "rimraf": "2.3.x",
    "rollup": "0.21.x",
    "sanctuary": "0.7.x",
    "testem": "0.9.x",
    "uglify-js": "2.4.x",
    "xyz": "0.5.x"
  }
}
